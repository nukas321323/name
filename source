local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SYX", "LightTheme")  ---if you want a diffrent theme pick here! (DarkTheme,GrapeTheme,BloodTheme,Ocean,Midnight,Sentinel,Synapse)

-- MAIN --
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("!Instructions!")

-- Scripts --

local Scripts = Window:NewTab("Scripts")
local ScriptsSection = Scripts:NewSection("!Main Scripts!")

-- Script --

ScriptsSection:NewButton("Andromeda", "Auto Farm for bee swarm simulator", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/BztZ6kyZ"))()
end)

ScriptsSection:NewButton("Kocmoc", "Auto Farm for bee swarm simulator", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Boxking776/kocmoc/main/kocmoc-remastered.lua"))()
end)

-- Label --

ScriptsSection:NewLabel("ANDROMEDA AND KOCMOC IS ONLY FOR BSS!")

-- Script --

ScriptsSection:NewButton("INF YIELD", "Free Admin", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

-- Toggle --

--LOCAL PLAYER
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewSlider("Walkspeed", "SPEED!!", 500, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("Jumppower", "JUMP HIGH!!", 350, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlayerSection:NewButton("Reset WS/JP", "Resets to all defaults", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)


--Scripts

ScriptsSection:NewButton("Chat Spoofer", "Lets you chat for other people", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()
end)

ScriptsSection:NewButton("Bypassed Fly", "bird mode", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Nicuse/RobloxScripts/main/BypassedFly.lua"))() 

    Fly(true)
end)

ScriptsSection:NewButton("FE Sonic", "SONIC MODE", function()
    loadstring(game:HttpGetAsync("https://pastebin.com/raw/SyF5t70A"))()
end)

ScriptsSection:NewButton("FE Walk", "SPIDER MODE", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/geUZHr7B"))()
end)

ScriptsSection:NewButton("Troll MENU", "LOL NUB", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Blukez/Scripts/main/UTG%20V3%20RAW"))()
end)


--Instructions--
MainSection:NewLabel("Hello! if you executed this script Please red this!")
MainSection:NewLabel("If you want to execute thhe main scripts please click on the Script TAB!")

-- Credits --
local Credits = Window:NewTab("Credits")
CreditsSection:NewLabel("Main Credits are for SYX#7873")

